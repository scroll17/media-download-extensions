{
  "name": "Media Donwload Extensions",
  "version": "0.0.1",
  "author": "Denis Zolotarenko",
  "manifest_version": 2,
  "browser_action": {
      "default_popup": "index.html",
      "default_title": "Donwload",
      "default_icon": "icon.png"
  },
  "icons": {
      "256": "icon.png"
  },
  "background": {
    "persistent": false,
    "page": "index.html" 
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["static/js/2.ef41c53d.chunk.js"],
      "css": ["static/css/main.78247fc1.chunk.css"]
    }
  ],
  "content_security_policy": "script-src 'self' 'sha256-YWrZAZMzkC/1bIirNxCX5kYbc+EouyDot6B2r8CAxBQ=' https://kit.fontawesome.com/8a98030ce8.js; object-src 'self'",
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>",
    "storage"
  ]
}
