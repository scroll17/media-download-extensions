#! /usr/bin/env node
const [{ execMigrate }] = require('./transpileTS')([
  '../src/db/migration/index.ts',
]);

console.info('Preparing migrationsâ€¦');
execMigrate(process.argv[2], process.argv[3], process.argv.slice(4))
  .then(() => process.exit(0))
  .catch(error => {
    console.error('migrate error: ', error);
    process.exit(1);
  });
